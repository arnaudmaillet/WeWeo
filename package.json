    "@react-native-firebase/firestore": "^21.4.1",
    "@react-native-picker/picker": "2.9.0",
    "@react-navigation/native": "^6.1.7",
    "expo": "^52.0.6",
    "expo-constants": "~17.0.3",
    "expo-crypto": "~14.0.1",
    "expo-dev-client": "~5.0.1",
    "expo-file-system": "~18.0.3",
